{
  "name": "course-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/src/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/index.js\"",
    "integration": "docker compose -f docker-compose.integration.yaml down -v && docker compose -f docker-compose.integration.yaml up --build --force-recreate --abort-on-container-exit --exit-code-from app",
    "integration:jest": "jest test/integration --detectOpenHandles",
    "prisma:generate_types": "pnpm prisma generate --schema=../../packages/prisma/schema.prisma"
  },
  "prisma": {
    "seed": "pnpm exec ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/morgan": "^1.9.9",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "module-alias": "^2.2.3",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "supabase": "^1.127.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "@controllers": "dist/src/controllers",
    "@models": "dist/src/models",
    "@routes": "dist/src/routes",
    "@utils": "dist/src/utils",
    "@root": "."
  }
}
